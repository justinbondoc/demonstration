<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="2"
  SkinType="Component"
  Comment="HTML5 Toolbar skin"
  ComponentType="TopicToolbar"
  EnableResponsiveOutput="True">
  <Stylesheet>
    <Styles>
      <Style
        Name="Toolbar">
        <Properties>
          <Property
            Name="MarginBottom">33px</Property>
          <Property
            Name="MarginRight">-4px</Property>
          <Property
            Name="Float">right</Property>
        </Properties>
      </Style>
      <Medium
        Name="Mobile">
        <Style
          Name="Toolbar">
          <Properties>
            <Property
              Name="Display">none</Property>
          </Properties>
        </Style>
      </Medium>
      <Style
        Name="ToolbarItem">
        <Properties>
          <Property
            Name="BorderTop">none 1px #e1e1e1</Property>
          <Property
            Name="BorderRight">none 1px #e1e1e1</Property>
          <Property
            Name="BorderBottom">none 1px #e1e1e1</Property>
          <Property
            Name="BorderLeft">none 1px #e1e1e1</Property>
          <Property
            Name="BackgroundGradient">vertical transparent transparent</Property>
        </Properties>
      </Style>
    </Styles>
  </Stylesheet>
  <TopicToolbar
    EnableCustomLayout="true"
    Buttons="TopicRatings|Separator|EditUserProfile|Print|ExpandAll" />
  <Strings />
</CatapultSkin>